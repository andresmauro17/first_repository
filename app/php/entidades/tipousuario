<?php 

//esta clase tiene metodos para get y setear los parametros de los usuarios 
// se hace un mapeo de las propiedades de la tabla empresa

class tipousuario{
	public $idTipoUsuario;          
	public $nombre;
	
	function get_idTipoUsuario(){
		return $this->idTipoUsuario;/*siempre los atributos se llaman sin el caracter $ */
	}
	function set_idTipoUsuario($idTipoUsuario){/*asigna un valor al atributo*/
		$this->idTipoUsuario=$idTipoUsuario;
	}
	function get_nombre(){/*obtiene el valor del atributo*/
		return $this->nombre;
	}
	function set_nombre($nombre){/*asigna un valor al atributo*/
		$this->nombre = $nombre;
	}	
}

?>